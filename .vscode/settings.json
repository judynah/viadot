{
    "autoDocstring.docstringFormat": "google",
    "python.formatting.provider": "none",
    "python.formatting.blackArgs": [
        "--line-length",
        "88"
    ],
    "python.linting.enabled": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveTimeout": 3000,
    "cSpell.userWords": [
        "azstorage",
        "azuresql",
        "conda",
        "config",
        "dask",
        "data",
        "distributed",
        "dotenv",
        "dtype",
        "dtypes",
        "dyvenia",
        "entrypoints",
        "gmtime",
        "home",
        "http",
        "itertools",
        "lib",
        "odbc",
        "pgadmin",
        "pgdata",
        "postgres",
        "postgresql",
        "prefect",
        "projects",
        "pyarrow",
        "pyodbc",
        "pytest",
        "pytesting",
        "repos",
        "scheduler",
        "supermetrics",
        "tabularize",
        "toml",
        "traefik",
        "users",
        "var",
        "work",
        "zfill"
    ],
    "git.autofetch": true,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    }
}